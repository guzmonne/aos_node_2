<div class="col-lg-12 well" id="appliance-{{index}}" style="border-radius: 0;{{style}}"> 
	<div class="row">  
		<div class="col-xs-10"> 
			<h4> 
				Equipo #{{sum index 1}} 
			</h4> 
		</div> 
		<div class="col-xs-2"> 
			<button class="btn btn-danger btn-small pull-right appliance-delete" data-index="{{index}}" tabindex="-1"> 
				<i class="fa fa-times"></i> 
			</button> 
		</div> 
		<div class="col-xs-12"><hr style="margin: 0"></div> 
	</div> 
	<div class="row" id="appliance-container-{{index}}"></div> 	
</div> 