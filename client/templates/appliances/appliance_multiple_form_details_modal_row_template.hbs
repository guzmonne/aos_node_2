<tr data-row={{row}}>
	<td name="id">{{id}}</td>
	<td>
		<input type="text" class="form-control" name="serial" value="{{serial}}" style="width: 100%">
	</td>
	<td>
		<select type="text" class="form-control" name="accessories" multiple style="width: 100%">
		{{#each accessories}} <option value="{{this}}"></option> {{/each}}
	</td>
	<td>
		<textarea class="form-control" name="observations" rows="1" style="width: 100%;height: 34px;padding: 6px 12px;">{{observations}}</textarea>
	</td>
	<td>
		<textarea class="form-control" name="defect" rows="1" style="width: 100%;height: 34px;padding: 6px 12px;">{{defect}}</textarea>
	</td>
</tr>