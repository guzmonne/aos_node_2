<div class="col-lg-12 hide" id="report-buttons">
	<span id="printAppliancesPDF" class=" pull-right btn btn-info btn-sm btn-margin" data-toggle="tooltip" data-placement="top" title="Imprimir Equipos" style="margin-bottom: 5px;">
    <i class="fa fa-print fa-fw fa-lg"></i>
  </span>
  <span id="downloadAppliancesPDF" class=" pull-right btn btn-info btn-sm btn-margin" data-toggle="tooltip" data-placement="top" title="Descargar Equipos" style="margin-bottom: 5px;">
    <i class="fa fa-download fa-fw fa-lg"></i>
  </span>
</div>
<div class="col-lg-12">
	<table id="appliances-table-{{timestamp}}" class="table table-striped table-bordered table-condensed table-hover table-office table-condensed" aria-describedby="appliances-table-info">
		<thead>
			<tr>
				<th>ID</th>
				<th>Cliente</th>	
				<th>Modelo</th>
				<th>Marca</th>
				<th>Serie</th>
				<th>Tipo</th>
				<th>Estado</th>
				<th>Tecnico</th>
				<th>Creado</th>
				<th>Cerrado</th>
				<th class="text-center"><i class="fa fa-wrench"></i></th>
			</tr>
		</thead>
		<tbody role="alert" aria-live="polite" aria-relevant="all" id="appliances">
		</tbody>
	</table>
</div>
