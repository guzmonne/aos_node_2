<td>
	<dd><i class="fa fa-user fa-fw"></i> {{name}}</dd>
	<dd><i class="fa fa-envelope fa-fw"></i> {{email}}</dd>
</td>
<td>
	<dt>Roles</dt>
	{{#if permissions.roles.isAdmin}}<dd>Administrador</dd>{{/if}}
	{{#if permissions.roles.isTech}}<dd>Tecnico</dd>{{/if}}
</td>
<td class="center-vh">
	<a href="#render/user/show/{{_id}}" class="btn btn-green" data-toggle="tooltip" data-placement="top" title="Más Información" id="show">
		<i class="fa fa-ellipsis-h fa-lg"></i>
	</a>
	<a id="selected" class="btn btn-warning hide" data-toggle="tooltip" data-placement="top" title="Seleccionar">
		<i class="fa fa-external-link fa-lg"></i>
	</a>
</td>