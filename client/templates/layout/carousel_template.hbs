<div id="carousel-{{cid}}" class="col-lg-12 carousel slide">
	<div class="row well well-sm" style="border-radius: 0">
		<div class="col-xs-12">
			<h2 class="text-center" style="margin-top: 10px">
				<a href="#carousel-{{cid}}" id="prev-model" class="btn btn-green pull-left" data-slide="prev">
					<i class="fa fa-chevron-left"></i>
				</a>
				{{carouselTitle}}
				<a href="#carousel-{{cid}}" id="next-model" class="btn btn-green pull-right" data-slide="next">
					<i class="fa fa-chevron-right"></i>
				</a>
			</h2>
		</div>
	</div>
	<div class="row">
		<div class="{{carouselClassName}}">
			<div id="carousel-items-{{cid}}" class="carousel-inner"></div>
		</div>
	</div>
</div>